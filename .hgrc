[ui]
username = noahsark
editor = /usr/bin/vim

[defaults]
log = --color always
diff = --color auto

[extensions]
color =
fetch =
convert =
graphlog =
bookmarks =
mq =
record =
hgattic = ~/workspace/config/hgshelve.py
rebase =
transplant =
prompt = ~/workspace/config/prompt.py
#hgshelve = ~/workspace/config/hgshelve.py

[bookmarks]
track.current = True

[trusted]
users = hg

[tortoisehg]
editor = /usr/bin/gvim

[extdiff] 
cmd.vdiff = kdiff3 
